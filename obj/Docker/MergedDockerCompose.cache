services:
  digital-bank-api:
    build:
      context: C:\Users\jgomes\source\repos\DigitalBank
      dockerfile: src/DigitalBank.Services.Api/Dockerfile
    container_name: digital-bank-api
    image: digital-bank-api
    ports:
    - published: 5000
      target: 80
  mongo:
    container_name: digital-bank-mongo
    image: mongo
    ports:
    - published: 27017
      target: 27017
  mongo-express:
    container_name: digital-bank-mongo-express
    depends_on:
    - mongo
    image: mongo-express
    ports:
    - published: 8081
      target: 8081
version: '3.4'